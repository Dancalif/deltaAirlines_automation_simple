   <test name="FlightStatusTab Firefox Test">
  <parameter name="browser" value="firefox" />
  <parameter name="url" value="https://www.delta.com" />
  <parameter name="flightNumber" value="2049" />
    <classes>
      <class name="test.FlightStatusTab_Test" />
    </classes>
  </test>
  
</suite>