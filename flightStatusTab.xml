<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="FlightStatusTab Suite" parallel="tests" thread-count="1">

 <test name="Chrome Test">
  <parameter name="browser" value="chrome" />
  <parameter name="url" value="https://www.delta.com" />
  <parameter name="flightNumber" value="2049" />
